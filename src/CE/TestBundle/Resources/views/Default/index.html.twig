{% extends "CETestBundle::layout.html.twig" %}
{% block stylesheets%}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset("bundles/cetest/css/weeklyCalendar.css") }}">
{% endblock %}
{% block headjs %}
    {{ parent() }}
    <script src="{{ asset("bundles/cetest/js/weeklyCalendarGenerator.js") }}"></script>
{% endblock %}
{% block  body%}
